{
	"name": "epark插件",
	"version": "1.0.0.1",
	"manifest_version": 2,
	"description": "linghaisen，493101885@qq.com",
	"options_page": "html/options.html",
	"permissions": [
		"management",
		"storage",
		"tabs",
		"unlimitedStorage",
		"notifications",
		"http://*/*",
		"https://*/*"
	],
	"background": {
		"scripts": [
			"js/account.js",
			"js/jquery-1.11.1.js",
			"js/background.js"
		]
	},
	"browser_action": {
		"default_icon" : "image/demon-19.png",
		"default_title": "epark插件\u2122",
		"default_popup": "html/popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"include_matches": [
				"http://htc.taizhou.gov.cn/*/*",
				"http://www.teyiting.com/*/*",
				"http://localhost:8080/*/*",
				"http://jtss.rzbus.cn:980/*/*",
				"http://www.cszhtc.com/*/*",
				"http://www.topeasypark.com/*/*",
				"http://localhost:8080/*/*/*"
			],
			"js": [
				"js/account.js",
				"js/jquery-1.11.1.js",
				"js/content.js"
			],
			"css": [
				"css/content.css"
			],
			"run_at": "document_start",
			"all_frames": true
		}
	],
	"icons": {
		"16": "image/demon-16.png",
		"48": "image/demon-48.png",
		"128": "image/demon-128.png"
	}
}
